<script type="text/javascript">  

	$(function() {
		var reason = $( "#reportReason" ),
			additionalinfo = $( "#insertDescription" ),
			allFields = $( [] ).add( reason ).add( additionalinfo ),
			tips = $( ".validateTips" );
			
		$( "#reportDialog" ).dialog({
			autoOpen: false,
			height: 400,
			width: 550,
			modal: true,
			buttons: {
				"Flag This Material": function() {
					
					var bValid = true;
					bValid = bValid && (reason.val() != 0);
					
					
					if ( bValid ) {						
						$.post( "http://localhost/studygig/index.php/flags/add", { reportReason: reason.val(), 
							reportAdditionalInfo : additionalinfo.val(),
							upload_id: "49",
							type: 0},
					      function( data ) {
					      	 $("#ratingFeedback").css("visibility","visible");
					      	document.getElementById("ratingFeedback").innerHTML=data;
					      }
					    );
						$( this ).dialog( "close" );
					}
				},
				Cancel: function() {
					$( this ).dialog( "close" );
				}
			},
			close: function() {
				allFields.val( "" ).removeClass( "ui-state-error" );
			}

		});

		$( ".reportIconView" ).click(function() {
			$( "#reportDialog" ).dialog( "open" );
			return false;
		});
	});

  function loadGoodVote(){
  	/* attach a submit handler to the form */
  $("#ratingForm").submit(function(event) {
	
    /* stop form from submitting normally */
    event.preventDefault(); 
  });
  	
  	 /* Send the data using post and put the results in a div */
    $.post( "http://localhost/studygig/index.php/ratings/add", { rating_type: "1", upload_id: "49"},
      function( data ) {
      	$("#ratingFeedback").css("visibility","visible");
      	document.getElementById("ratingFeedback").innerHTML=data;
         $(".ratingIconGood").css("background-position","left bottom");
         $(".ratingIconGood").css("color","#009b00");
         $('.ratingIconGood').attr('value', ' 1');
      }
    );
  }
  
    function loadBadVote(){
  	/* attach a submit handler to the form */
  $("#ratingForm").submit(function(event) {
	
    /* stop form from submitting normally */
    event.preventDefault(); 
  });
  	
  	 /* Send the data using post and put the results in a div */
    $.post( "http://localhost/studygig/index.php/ratings/add", { rating_type: "0", upload_id: "49"},
      function( data ) {
      	$("#ratingFeedback").css("visibility","visible");
      	document.getElementById("ratingFeedback").innerHTML=data;
         $(".ratingIconBad").css("background-position","left bottom");
         $(".ratingIconBad").css("color","#9b0000");
         $('.ratingIconBad').attr('value', ' 1');
      }
    );
  }
  

